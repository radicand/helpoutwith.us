{
  "name": "howus",
  "version": "1.0.0",
  "description": "Code for the HOWUS site",
  "engines": {
    "node": ">=8.9.1"
  },
  "scripts": {
    "updateGraphSchema": "npm run generateSchema && npm run generateSchemaTypes",
    "generateSchema": "apollo schema:download --endpoint $GC_URL src/queries/schema.json",
    "generateSchemaTypes": "apollo codegen:generate --addTypename --queries='src/**/*.graphql' --schema=src/queries/schema.json --target typescript --outputFlat src/queries/schema.ts",
    "cleanup": "rimraf dist",
    "bundle:server": "cross-env NODE_ENV=production node fuse server",
    "bundle:client": "cross-env NODE_ENV=production node fuse",
    "predev": "yarn cleanup",
    "dev": "yarn updateGraphSchema && yarn bundle:server && cross-env WEB_ROOT=dist/public node fuse",
    "preprod": "yarn cleanup",
    "prod": "yarn cleanup && yarn updateGraphSchema && yarn bundle:server && yarn bundle:client",
    "start": "cross-env NODE_ENV=production WEB_ROOT=dist/public node dist/server",
    "predocker:build": "yarn prod",
    "docker:build": "docker build -t howus .",
    "docker:deploy": "docker tag howus:latest $REMOTE_DOCKER_PATH/howus:latest && docker push $REMOTE_DOCKER_PATH/howus:latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/radicand/helpoutwith.us"
  },
  "keywords": [
    "hapi",
    "react",
    "reactjs",
    "hot",
    "hmr",
    "fuse-box",
    "howus"
  ],
  "author": "radicand",
  "license": "Apache-2.0",
  "dependencies": {
    "@date-io/luxon": "0.0.2",
    "@material-ui/core": "3.5.1",
